[ignore]
.*/node_modules/bcrypt/.*
.*/node_modules/webpack-cli/.*
.*/node_modules/webpack-hot-middleware/.*

[include]

[libs]
./src/type-def-libs

[lints]
sketchy-null=warn
sketchy-null-bool=off

[options]
suppress_type=warn
; Extensions
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.scss
module.file_ext=.html

[strict]
unclear-type
untyped-import
untyped-type-import